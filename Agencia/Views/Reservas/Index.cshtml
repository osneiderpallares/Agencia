@model IEnumerable<Agencia.Models.Reservas>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alojamiento1.ciudadDestino)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contactoEmergencia1.nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estados.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alojamiento1.ciudadDestino)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contactoEmergencia1.nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estados.nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
